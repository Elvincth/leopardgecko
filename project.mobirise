          ],
          "_name": "content5",
          "_anchor": "content5-z",
          "_params": {
            "paddingTop": "2",
            "paddingBottom": "0",
            "bgImageRadio": true,
            "parallax": false,
            "bgImage": "@PROJECT_PATH@/assets/images/pastel-2000x1213.png",
            "bgColor": "#fff",
            "bgColorRadio": false,
            "bgVideo": {
